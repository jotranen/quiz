{
  "name": "party-party",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc && mkdir -p ./dist/schema",
    "start": "node ./dist/server.js",
    "tsc": "tsc",
    "test": "mocha --recursive --exit --require ts-node/register 'src/**/*.test.ts'",
    "generate": "ts-node --transpile-only src/schema.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "prod": "tsc && node ./dist/index.js"
  },
  "dependencies": {
    "@koa/cors": "^3.4.1",
    "apollo-server": "^3.10.2",
    "apollo-server-koa": "^3.10.2",
    "graphql": "^16.6.0",
    "gulp": "^4.0.2",
    "knex": "^2.3.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.0",
    "koa-swagger-decorator": "^1.8.6",
    "nexus": "^1.3.0",
    "parse-database-url": "^0.3.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.8",
    "@types/koa-helmet": "^6.0.4",
    "@types/koa-json": "^2.0.20",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-router": "^7.4.4",
    "@types/koa__cors": "^3.3.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.18",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3"
  }
}
